{
  "index":{
     "solr":{ 
        "url":"http://127.0.0.1:8080/${project.build.finalName}/solr/psq-01",
        "shard":["127.0.0.1:8080/${project.build.finalName}/solr/psq-01",
                 "127.0.0.1:8080/${project.build.finalName}/solr/psq-02",
                 "127.0.0.1:8080/${project.build.finalName}/solr/psq-03"]
       },
      "transform":{
          "mif254":{
             "type":"xslt", 
             "config":"../etc/ic-psq/server/solr/config/mif2index.xml"
          }
      }
  },
  "store":{
     "derby":{
        "derby-db":"/opt/psq-data/psq-derby"
     },
     "record-mgr": "http://localhost:8080/ic-psq-server/recordmgr",
     "transform":{
        "mif254":[
            { "format":"psi-mi/tab25", 
               "type":"xslt", 
               "config":"../etc/ic-psq/server/solr/config/mif2mitab25view.xml"
            },
            { "format":"psi-mi/tab27", 
               "type":"xslt", 
               "config":"../etc/ic-psq/server/solr/config/mif2mitab25view.xml"
            },
            { "format":"psi-mi/xml25", 
               "type":"xslt", 
               "config":"../etc/ic-psq/server/solr/config/mif2mitab25view.xml"
            }
        ]
     }
  },
  "service":{
     "soap":{
        "version":"1.1",
        "properties":{
           "implementation":"ic-psq-0.01",
           "last-protein-update":"01-Jan-12"
          },
        "supported-return-type":[
           "psi-mi/tab25",
           "psi-mi/tab27",
           "psi-mi/xml25" ],
        "supported-db-ac":[ 
           "MI:0670", 
           "MI:0481", 
           "MI:0486" ]
     },
     "rest":{
        "version":"1.1",
        "properties":{},
        "supported-return-type":[
           "psi-mi/tab25",
           "psi-mi/tab27",
           "psi-mi/xml25" ],
        "supported-db-ac":[
           "MI:0670",
           "MI:0481",
           "MI:0486" ]
     }
  }
}

