    <groupId>edu.ucla.mbi.imex</groupId>
    <artifactId>ic-persist</artifactId>
    <packaging>jar</packaging>
<!--    <version>3.0.0rc4</version> -->
    <name>Persistence module</name>

    <parent>
      <groupId>edu.ucla.mbi.imex</groupId>
      <artifactId>imexcentral</artifactId>
      <version>3.0.0rc8</version>
    </parent>

    <properties>
