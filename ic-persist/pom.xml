<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>
    <groupId>edu.ucla.mbi.imex</groupId>
    <artifactId>ic-persist</artifactId>
    <packaging>jar</packaging>
    <version>1.0-SNAPSHOT</version>
    <name>Persistence module</name>

    <parent>
      <groupId>edu.ucla.mbi.imex</groupId>
      <artifactId>imexcentral-project</artifactId>
      <version>1.0-SNAPSHOT</version>
    </parent>

    <dependencies>

     <dependency>
      <groupId>edu.ucla.mbi.imex</groupId>
      <artifactId>ic-model</artifactId>
      <version>1.0-SNAPSHOT</version>
     </dependency>

     <dependency>
      <groupId>edu.ucla.mbi.dip</groupId>
      <artifactId>dip-orm</artifactId>
      <version>0.01</version>
     </dependency>

     <dependency>
      <groupId>edu.ucla.mbi.util</groupId>
      <artifactId>dip-util-data</artifactId>
      <version>1.0-SNAPSHOT</version>
     </dependency>

     <dependency>
      <groupId>edu.ucla.mbi.dip</groupId>
      <artifactId>dip-util-struts</artifactId>
      <version>0.01</version>
     </dependency>

     <dependency>
      <groupId>com.sun.xml.ws</groupId>
      <artifactId>jaxws-rt</artifactId>
      <version>2.1.3</version>
     </dependency>
     
    </dependencies>

    <build>
     <finalName>ic-persist</finalName>

     <plugins>
      <plugin>
       <groupId>org.codehaus.mojo</groupId>
       <artifactId>jaxws-maven-plugin</artifactId>
      <executions>
       <execution>
         <id>ncbi-proxy</id>
         <goals>
          <goal>wsimport</goal>
         </goals>
         <configuration>
          <verbose>false</verbose>
          <bindingFiles>
           <bindingFile>DXF1.4.jxb</bindingFile>
           <bindingFile>services.jxb</bindingFile>
           <bindingFile>NcbiService14.jxb</bindingFile>
          </bindingFiles>
          <wsdlFiles>
            <wsdlFile>NcbiService14.wsdl</wsdlFile>
          </wsdlFiles>
          <wsdlLocation>NcbiService14.wsdl</wsdlLocation>
          <staleFile>${basedir}/target/.ncbi-proxy.staleFlag</staleFile>
          <keep>true</keep>
         </configuration>
        </execution>
      </executions>
    </plugin>

   </plugins>
  </build>
</project>
