     <dependency>
      <groupId>edu.ucla.mbi.imex</groupId>
      <artifactId>ic-model</artifactId>
      <version>3.0.0rc8</version>
     </dependency>

     <dependency>
      <groupId>edu.ucla.mbi.dip</groupId>
      <artifactId>dip-util-orm</artifactId>
      <version>${dip-util.version}</version>
     </dependency>

     <dependency>
      <groupId>edu.ucla.mbi.dip</groupId>
      <artifactId>dip-util-data</artifactId>
      <version>${dip-util.version}</version>
     </dependency>

     <dependency>
      <groupId>edu.ucla.mbi.dip</groupId>
      <artifactId>dip-util-struts</artifactId>
      <version>${dip-util.version}</version>
     </dependency>


     <dependency>
      <groupId>edu.ucla.mbi.dip</groupId>
      <artifactId>dip-util-ws</artifactId>
      <version>${dip-util.version}</version>
     </dependency>


     <dependency>
      <groupId>edu.ucla.mbi.dip</groupId>
      <artifactId>dip-proxy-api</artifactId>
      <version>${dip-proxy.version}</version>
     </dependency>


     <dependency>
      <groupId>com.sun.xml.ws</groupId>
      <artifactId>jaxws-rt</artifactId>
      <version>${jaxws-rt.version}</version>
     </dependency>
     
    </dependencies>

    <build>
     <finalName>ic-persist</finalName>

     <plugins>
      <plugin>
       <groupId>org.codehaus.mojo</groupId>
       <artifactId>jaxws-maven-plugin</artifactId>
       <version>2.4.1</version> 
      <executions>
       <execution>
         <id>ncbi-proxy</id>
         <goals>
          <goal>wsimport</goal>
         </goals>
         <configuration>
          <verbose>false</verbose>
          <bindingFiles>
           <bindingFile>DXF1.4.jxb</bindingFile>
           <bindingFile>services.jxb</bindingFile>
           <bindingFile>NcbiService14.jxb</bindingFile>
          </bindingFiles>
          <wsdlFiles>
            <wsdlFile>NcbiService14.wsdl</wsdlFile>
          </wsdlFiles>
          <wsdlLocation>NcbiService14.wsdl</wsdlLocation>
          <staleFile>${basedir}/target/.ncbi-proxy.staleFlag</staleFile>
          <keep>true</keep>
         </configuration>
        </execution>
      </executions>
    </plugin>

   </plugins>
  </build>
</project>
